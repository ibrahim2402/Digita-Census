CREATE TABLE census_table1 (
id INT UNSIGNED NOT NULL AUTO_INCREMENT PRIMARY KEY,
firstName VARCHAR(45) NOT NULL,
middleName VARCHAR(45) NOT NULL,
lastName VARCHAR(45) NOT NULL,
gender VARCHAR(10) NOT NULL,
dateOfBirth VARCHAR(25) NOT NULL,
ssn INT(10) UNSIGNED NOT NULL,
maritalStatus VARCHAR(10) NOT NULL,
numberOfChildren INT(11) NOT NULL,
placeOfBirth VARCHAR(45) NOT NULL,
religion VARCHAR(15) NOT NULL,
stateOfOrigin VARCHAR(15) NOT NULL,
address VARCHAR(45) NOT NULL,
address_city VARCHAR(45) NOT NULL,
address_state VARCHAR(45) NOT NULL,
address_postCode VARCHAR(10) NULL,
address_country VARCHAR(15) NOT NULL,
employment_status VARCHAR(25) NOT NULL,
employment_profession VARCHAR(45) NOT NULL,
nextOfKin_name VARCHAR(45) NOT NULL,
nextOfKin_address VARCHAR(45) NOT NULL,
nextOfKin_city VARCHAR(45) NOT NULL,
nextOfKin_state VARCHAR(45) NOT NULL,
nextOfKin_postCode VARCHAR(10) NULL,
nextOfKin_country VARCHAR(25) NOT NULL,
nextOfKin_phone VARCHAR(25) NOT NULL,
nextOfKin_email VARCHAR(45) NOT NULL,
houseHold_id INT (10) UNSIGNED NOT NULL,
houseHold_role VARCHAR(45) NOT NULL,
census_id VARCHAR(45) NULL
);



